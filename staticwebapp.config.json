{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
    "route": "/admin/*",
      "allowedRoles": [ "admin" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    },
    "403": {
      "statusCode": 302,
      "redirect": "/unauthorised"
    }
  }
}